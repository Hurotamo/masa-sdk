{
  "name": "@masa-finance/masa-sdk",
  "version": "0.1.0",
  "description": "Brand new masa SDK",
  "main": "dist/src/index.js",
  "module": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "tsc --watch",
    "build": "tsc",
    "prettier": "prettier ./src --write",
    "lint": "eslint ./src",
    "doc": "typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts --name '# Masa SDK\n'",
    "publish-lib": "npm run build && npm publish",
    "publish-beta": "npm run build && npm publish --tag beta",
    "publish-dryrun": "npm run build && npm publish --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masa-finance/masa-sdk.git"
  },
  "keywords": [
    "De-fi"
  ],
  "author": "Masa Finance",
  "license": "MIT",
  "dependencies": {
    "@masa-finance/masa-contracts-identity": "^0.3.1",
    "arweave": "^1.11.6",
    "axios": "^1.1.3",
    "ethers": "^5.7.2",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "prettier": "^2.7.1",
    "typedoc": "^0.23.18",
    "typedoc-plugin-markdown": "^3.13.6"
  }
}
