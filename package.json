{
  "name": "@masa-finance/masa-sdk",
  "version": "3.2.0-alpha-1",
  "description": "Brand new Masa SDK",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "unpkg": "./dist/browser/masa-sdk.cjs2.min.js",
  "module": "./dist/browser/masa-sdk.amd.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ts-mocha test/**/*.test.ts",
    "test:watch": "ts-mocha --extension ts,js test/**/*.test.ts --watch",
    "start": "yarn build:watch",
    "build": "yarn build:tsc && yarn build:dist",
    "build:tsc": "tsc",
    "build:dist": "cross-env NODE_ENV=production webpack",
    "build:watch": "tsc -w",
    "prettier": "prettier ./src ./test --write",
    "lint": "eslint ./src ./test",
    "doc": "typedoc",
    "prepublishOnly": "yarn build",
    "publish-lib": "npm publish",
    "publish-beta": "npm publish --tag beta",
    "publish-dryrun": "npm publish --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masa-finance/masa-sdk.git"
  },
  "keywords": [
    "De-fi"
  ],
  "author": "Masa Finance",
  "license": "MIT",
  "dependencies": {
    "@masa-finance/masa-contracts-identity": "^1.8.0",
    "@masa-finance/masa-token": "^2.0.0",
    "arweave": "1.11.8",
    "axios": "^1.3.2",
    "ethers": "~5.7.2",
    "grapheme-splitter": "^1.0.4",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-plugin-import": "^2.27.5",
    "mocha": "^10.2.0",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-mocha": "^10.0.0",
    "typedoc": "^0.24.6",
    "typedoc-plugin-markdown": "^3.15.2",
    "webpack": "^5.84.0",
    "webpack-cli": "^5.0.1"
  }
}
